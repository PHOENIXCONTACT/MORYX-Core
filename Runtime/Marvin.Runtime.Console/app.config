<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- Register Common Logging Section-->
    <section name="commonLogging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
  </configSections>
  <!-- Common Logging Configuration -->
  <commonLogging>
    <!-- using log4net 1.2.10.0 as Logging tool -->
    <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1210">
      <arg key="configType" value="FILE-WATCH" />
      <!-- log4net as configured in a separate file -->
      <arg key="configFile" value="~/config/log4net.config" />
    </factoryAdapter>
  </commonLogging>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Threading.Tasks.Extensions" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.1.1.0" newVersion="4.1.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Npgsql" publicKeyToken="5d8b90d52f46fda7" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.2.6.0" newVersion="3.2.6.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>